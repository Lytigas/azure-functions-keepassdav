{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "dav/{filename}",
      "methods": ["delete", "put", "move", "get"],
      "dataType": "binary"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
